<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.SplitPane?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox prefHeight="600.0" prefWidth="1000.0" xmlns="http://javafx.com/javafx/17.0.6" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.mainController">
  <children>
    <SplitPane focusTraversable="true" prefHeight="-1.0" prefWidth="-1.0" VBox.vgrow="ALWAYS">
      <items>
        <AnchorPane prefHeight="500.0" prefWidth="700.0" style="-fx-background-color: #0589ff;">
          <children>
            <Label alignment="CENTER" layoutX="64.0" layoutY="38.0" minWidth="60.0" prefHeight="35.0" prefWidth="87.0" style="&#10;" text="Home" textAlignment="CENTER" textFill="WHITE" wrapText="false">
              <font>
                <Font name="System Bold" size="24.0" fx:id="x1" />
              </font>
            </Label>
                  <Button fx:id="profileBtn" layoutX="64.0" layoutY="126.0" mnemonicParsing="false" onAction="#switchProfile" prefHeight="40.0" prefWidth="100.0" text="Profile" />
                  <Button fx:id="myEventsBtn" layoutX="64.0" layoutY="260.0" mnemonicParsing="false" onAction="#switchMyEvents" prefHeight="40.0" prefWidth="100.0" text="Events" />
                  <Button fx:id="eventBtn" layoutX="64.0" layoutY="194.0" mnemonicParsing="false" onAction="#switchEvents" prefHeight="40.0" prefWidth="100.0" text="Explore" />
                  <StackPane fx:id="pageHolder" layoutX="197.0" prefHeight="600.0" prefWidth="730.0" />
                  <Button layoutX="78.0" layoutY="529.0" mnemonicParsing="false" onAction="#logout" prefHeight="20.0" prefWidth="71.0" text="Log Out" />
                  <Button layoutX="64.0" layoutY="432.0" mnemonicParsing="false" onAction="#switchCreateEvent" prefHeight="40.0" prefWidth="100.0" text="Create Event" />
          </children>
        </AnchorPane>
      </items>
    </SplitPane>
  </children>
</VBox>
